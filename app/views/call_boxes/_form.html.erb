<%= semantic_form_for([@layer, @call_box], :html => { :class => 'cmxform'} ) do |f| %>
  <% f.inputs do %>
    <%= f.input :cb_num %>
    <%= f.input :cb_type %><br />
    <%= f.input :latitude %><br />
    <%= f.input :longitude %><br />
    <%= f.input :aproximate %>
    <%= f.input :layer_id %>
    <%= f.input :notes %><br />
  <% end %>
  <% f.actions do %>
    <%= f.action :submit %>
  <% end %>
<% end %>
